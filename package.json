{
  "name": "release",
  "version": "0.1.0",
  "description": "Create GitHub Releases from the command line",
  "scripts": {
    "test": "xo"
  },
  "repository": "zeit/release",
  "keywords": [
    "github",
    "releases",
    "cli",
    "tags",
    "push"
  ],
  "author": "leo",
  "license": "MIT",
  "bin": {
    "release": "./index.js"
  },
  "xo": {
    "esnext": true,
    "space": true,
    "semicolon": false,
    "rules": {
      "import/no-dynamic-require": 0,
      "unicorn/no-process-exit": 0,
      "camelcase": 0
    }
  },
  "bugs": {
    "url": "https://github.com/zeit/release/issues"
  },
  "homepage": "https://github.com/zeit/release#readme",
  "devDependencies": {
    "xo": "^0.17.1"
  },
  "dependencies": {
    "args": "^2.2.1",
    "capitalize": "^1.0.0",
    "chalk": "^1.1.3",
    "git-commits": "^1.2.0",
    "github": "^7.1.0",
    "github-url-to-object": "^2.2.6",
    "inquirer": "^2.0.0",
    "is-url": "^1.2.2",
    "ora": "^0.4.0",
    "pluralize": "^3.1.0",
    "semver": "^5.3.0",
    "trim": "0.0.1"
  }
}
